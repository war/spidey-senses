{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SpiderControl": {
    "ValidCommands": [ "F", "R", "L" ]
  },
  "coverage": {
    "excludeByFile": [
      "**/Program.cs",
      "**/Startup.cs",
      "**/*Migrations*/**"
    ],
    "excludeByAttribute": [
      "Obsolete",
      "GeneratedCodeAttribute",
      "CompilerGeneratedAttribute"
    ],
    "excludeFromReport": true,
    "include": [
      "[SpiderControl.WebApiV2.*]*"
    ],
    "exclude": [
      "[*Tests]*",
      "[*TestUtils]*"
    ],
    "includeTestAssembly": false,
    "skipAutoProps": true
  }
}
